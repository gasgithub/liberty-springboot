pipeline {
	agent any
 	stages {
 		stage('Build Automation') {
 			steps {
 				sh '''
 				echo "BUILD NUMBER: ${BUILD_NUMBER}"
 				cd liberty.springboot
 				mvn clean package 
 				'''
 			}
 		}
 		stage('Prepare artifacts') {
 			steps {
 				sh '''
 				cd liberty.springboot/target
 				ls
 				archiveArtifacts 'liberty.springboot/target/*.jar, liberty.springboot/target/*.zip, liberty.springboot/target/*.war' 
 				'''
 			}
 		}
 	}
}