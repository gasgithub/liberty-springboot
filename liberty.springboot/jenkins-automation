pipeline {
	agent any
 	stages {
 		stage('Build Automation') {
 			steps {
 				sh '''
 				echo "BUILD NUMBER: ${BUILD_NUMBER}"
 				cd liberty.springboot
 				mvn clean package
 				archiveArtifacts artifacts: '**/target/*.jar, **/target/*.zip, **/target/*.war', fingerprint: true 
 				'''
 			}
 		}
 		stage('Prepare artifacts') {
 			steps {
 				sh '''
 				cd liberty.springboot/target
 				archiveArtifacts artifacts: '*.jar, *.zip, *.war', fingerprint: true 
 				'''
 			}
 		}
 	}
}